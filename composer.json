{
    "name": "vesperabr/laravel-macros",
    "description": "A set of useful Laravel macros",
    "keywords": [
        "vespera",
        "laravel",
        "macros"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/vesperabr/laravel-macros",
    "authors": [
        {
            "name": "Ricardo Monteiro",
            "email": "ricardo.monteiro@vespera.com.br"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.20",
        "phpunit/phpunit": "^9.4.4"
    },
    "autoload": {
        "psr-4": {
            "Vespera\\LaravelMacros\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vespera\\LaravelMacros\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vespera\\LaravelMacros\\Support\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
